//fibonacci series starting with indexing 0 .

#include<stdio.h>

int fibo( int n)
{
    //base case1
    if(n==0)
    {
        return 0;
    }
    // base case 2
    if(n==1)
    {
        return 1;
    }

    //recursive call
    int ans = fibo(n-1)+fibo(n-2);

    return ans ;
}
int main()
{
    int n;
    printf("enter index of fibonacci series:");
    scanf("%d",&n);
    printf("%d",fibo(n));
    return 0;
}


